
setenv NTASK 6
setenv RES 360x300

setenv IO_TYPE netcdf

set NXGLOB = `echo $RES | sed s/x.\*//`
set NYGLOB = `echo $RES | sed s/.\*x//`

setenv BLCKX `expr $NXGLOB / 6`       # x-dimension of blocks ( not including )
setenv BLCKY `expr $NYGLOB / 1`       # y-dimension of blocks (  ghost cells  )

